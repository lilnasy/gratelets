    "license": "MIT",
    "author": {
        "name": "lilnasy",
        "email": "hi@arsh.sh"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lilnasy/gratelets.git",
        "directory": "packages/node-websocket"
    },
    "homepage": "https://github.com/lilnasy/gratelets/tree/main/packages/node-websocket",
    "keywords": [
        "withastro",
        "astro",
        "astro-integration",
        "astro-adapter",
        "websocket"
    ],
    "files": [
        "withastro/astro/packages/integrations/node/src"
    ],
    "exports": {
