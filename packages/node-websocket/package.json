        "./websocket": "./withastro/astro/packages/integrations/node/src/runtime/websocket.ts"
    },
    "dependencies": {
        "@astrojs/internal-helpers": "0.6.1",
        "@types/ws": "^8.18.1",
        "bufferutil": "^4.0.9",
        "send": "^1.2.0",
        "server-destroy": "^1.0.1",
        "ws": "^8.18.3"
    },
    "peerDependencies": {
        "astro": "^5.3.0"
    },
    "devDependencies": {
        "@types/send": "^0.17.4",
        "@types/server-destroy": "^1.0.4"
    },
    "scripts": {
        "clone": "git submodule update --init .",
        "create_patches": "cd withastro/astro && git format-patch tags/@astrojs/node@9.4.0 -o ../..",
        "load_patches": "git submodule update --init . && cd withastro/astro && git am ../../*.patch",
        "delete_all_changes_and_unload_patches": "cd withastro/astro && git reset --hard tags/@astrojs/node@9.4.0",
        "delete_all_changes_and_reload_patches": "cd withastro/astro && git reset --hard tags/@astrojs/node@9.4.0 && git am ../../*.patch",
        "test": "pnpm -w test node-websocket.test.ts"
    }
